{
	"name": "hodic/nette-example",
	"description": "Nette Example",
	"keywords": ["nette"],
	"type": "project",
	"license": ["MIT"],
	"require": {
		"php": ">= 7.4",
		"nette/application": "^3.1",
		"nette/bootstrap": "^3.1",
		"nette/caching": "^3.1",
		"nette/database": "^3.1",
		"nette/di": "^3.0",
		"nette/finder": "^2.5",
		"nette/forms": "^3.1",
		"nette/http": "^3.1",
		"nette/mail": "^3.1",
		"nette/robot-loader": "^3.3",
		"nette/security": "^3.1",
		"nette/utils": "^3.2",
		"latte/latte": "^2.9",
		"tracy/tracy": "^2.8",
		"ipub/visual-paginator": "@dev"
	},
	"require-dev": {
		"nette/tester": "^2.3",
		"symfony/thanks": "^1",
		"phpstan/phpstan-nette": "^1.0",
		"arxeiss/coding-standards": "^0.9.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"minimum-stability": "stable",
	"scripts": {
		"phpstan": "vendor/bin/phpstan analyse app",
		"phpcbf": "./vendor/bin/phpcbf --standard=./phpcs.xml"
	}
}
